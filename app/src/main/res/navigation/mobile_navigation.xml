<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="diego.jimenez.max.rivera.facultaddejurisprudenciaapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">

        <action android:id="@+id/haciaIngresoExpediente"
            app:destination="@+id/fragment_ingresoexpediente"
            />
    </fragment>



    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="diego.jimenez.max.rivera.facultaddejurisprudenciaapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="diego.jimenez.max.rivera.facultaddejurisprudenciaapp.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/fragment_ingresoexpediente"
        android:name="diego.jimenez.max.rivera.facultaddejurisprudenciaapp.fragment_ingresoexpediente"
        android:label="fragment_ingresoexpediente"
        tools:layout="@layout/fragment_ingresoexpediente">

    </fragment>

</navigation>